    if (areaId <= 20) {
      return { world: 1, name: "First World", emoji: "" };
    } else if (areaId <= 40) {
      return { world: 2, name: "Ethereal Realms", emoji: "" };
    } else if (areaId <= 60) {
      return { world: 3, name: "Cosmic Dimension", emoji: "" };
    } else if (areaId <= 80) {
      return { world: 4, name: "Transcendence Realm", emoji: "" };
    } else {
