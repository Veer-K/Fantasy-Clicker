  };

  const rollRarity = (): Rarity => {
    const random = Math.random() * 100;
    let cumulative = 0;
    
    for (const [rarity, chance] of Object.entries(rarityChances)) {
      cumulative += chance;
      if (random <= cumulative) {
        return rarity as Rarity;
      }
    }
    
    return 'Common';
  };

